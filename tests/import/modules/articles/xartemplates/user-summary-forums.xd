    <tr>
        <td><a href="#$link#">#$title#</a></td>
        <xar:if condition="!empty($comments)">
            <td>#$comments#</td>
        <xar:else />
            <td>&nbsp;</td>
        </xar:if>
        <xar:if condition="!empty($profile)">
            <td><a href="#$profile#" title="View Profile">#$author#</a></td>
        <xar:elseif condition="!empty($summary)" />
            <td>#$summary#</td>
        <xar:else />
            <td>#$author#</td>
        </xar:if>
        <xar:if condition="!empty($counter)">
            <td>#$counter#</td>
        <xar:else />
            <td>&nbsp;</td>
        </xar:if>
        <xar:if condition="!empty($notes)">
            <td>#$notes#</td>
        <xar:else />
            <td>#$date#</td>
        </xar:if>
    </tr>

