<xar:set name="compare">xarServerGetCurrentURL()</xar:set>
<xar:set name="stub">basename(xarServerGetCurrentURL())</xar:set>

<xar:if condition="$stub eq 'tab1'">
    <dd class="active">
<xar:else />
    <dd class="inactive">
</xar:if>
    <a href="#$current#/tab1">
        <xar:mlstring>
            Tab 1
        </xar:mlstring>
    </a>
</dd>    
<xar:if condition="$stub eq 'tab2'">
    <dd class="active">
<xar:else />
    <dd class="inactive">
</xar:if>
    <a href="#$current#/tab2">
        <xar:mlstring>
            Tab 2
        </xar:mlstring>
    </a>
</dd>    
