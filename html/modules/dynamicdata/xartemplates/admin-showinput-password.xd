<xar:set name="askagain"><xar:mlstring>Type again:</xar:mlstring></xar:set>
    <input type="password" name="#$name1#" value="#$value#" size="#$size#" maxlength="#$maxlength#" #$id# #$tabindex#/>
    &nbsp;&nbsp;&nbsp;&nbsp;#$askagain#
    <input type="password" name="#$name2#" value="#$value#" size="#$size#" maxlength="#$maxlength#" #$id# #$tabindex#/>
<br />
<xar:if condition="!empty($invalid)">
    <span class="xar-error">#$invalid#</span>
</xar:if>
