<xar:set name="askagain"><xar:mlstring>Type again:</xar:mlstring></xar:set>
    <input type="password" id="#$id#" name="#$name1#" value="#$value#" size="#$size#" maxlength="#$maxlength#" #$tabindex#/>
    &nbsp;&nbsp;&nbsp;&nbsp;#$askagain#
    <input type="password" id="#$id#_2" name="#$name2#" value="#$value#" size="#$size#" maxlength="#$maxlength#" #$tabindex#/>

<xar:if condition="!empty($invalid)">
    <span class="xar-error">#$invalid#</span>
</xar:if>
