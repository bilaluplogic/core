<h2>#$label#</h2>

<xar:if condition="!empty($objectid)">
    <xar:data-view object="$objectid" numitems="20" layout="list" />
<xar:elseif condition="!empty($itemtype)" />
    <xar:data-view module="$modid" itemtype="$itemtype" numitems="20" layout="list" />
<xar:else />
    <center>
        <xar:mlstring>Please select one of the</xar:mlstring>
        <a href="#xarModURL('dynamicdata','user','main')#"><xar:mlstring>available objects</xar:mlstring></a>
    </center>
</xar:if>

