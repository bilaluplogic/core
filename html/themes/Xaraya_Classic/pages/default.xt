    <tr>
        <td align="center" colspan="3">-->
            <!--xar:blockgroup name="top" id="top" /-->
        <!--</td>
    </tr>
-->
</table>
<BR />

<!-- start body -->

<table width="100%" border=0 cellpadding=0 cellspacing=0>
    <tr>
        <td width="18%" align=left valign=top>
            <table width="100%" border=0 cellpadding=0 cellspacing=0>
                <tr>
                    <td bgcolor="##f0f0f0">
                        <xar:blockgroup name="left" id="left" />
                    </td>
                </tr>
                <tr height=200>
                    <td background="themes/Xaraya_Classic/images/sideline.gif">
                        <img src="themes/Xaraya_Classic/images/dot.gif">
                    </td>
                </tr>
            </table> 
        <td width="1%" background="themes/Xaraya_Classic/images/vline.gif">   
        </td>
        <td width="60%" class=main_copy valign=top>
            <xar:blockgroup name="center" id="center" />
            <xar:module id="modulespace" main="true" />
        </td>
        <td width="1%">   
        </td>
        <td width="20%" align=left valign=top background="themes/Xaraya_Classic/images/side_all.gif">
            <xar:blockgroup name="right" id="right" />
        </td>
    </tr>
</table>
<table width="100%" border=0 cellpadding=0 cellspacing=0>
    <tr>
        <td align=right bgcolor="##f0f0f0">
            <b>#xarConfigGetVar('Site.Core.Slogan')#</b>
        </td>
    </tr>
</table>
<center>
<b>#xarConfigGetVar('Site.Core.CopyRight')#</b>
<br />
<br />
#xarConfigGetVar('Site.Core.Footer')#
</center>
</body>
</html>