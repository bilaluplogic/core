<?xar type="page" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
    <head>
		<xar:blockgroup name="header" id="header" />
        <title>#$tpl:pageTitle#</title>
        <base href="&xar-baseurl;index.php" />
        <link rel="StyleSheet" href="&xar-baseurl;themes/Xaraya_Classic/style/styleNN.css" type="text/css" />
        <style type="text/css">@import url(&xar-baseurl;themes/Xaraya_Classic/style/style.css); </style>
        #$tpl:additionalStyles#
        #$tpl:headJavaScript#
    </head>
    <body>
<table width="100%" cellpadding="0" cellspacing="0" border="0">
    <tr>
        <td style="text-align:left; background-image:url(themes/Xaraya_Classic/images/header_fill.gif)">
            <a href="&xar-baseurl;">
            <img src="themes/Xaraya_Classic/images/header_logo1.gif" alt="#xarModGetVar('themes', 'SiteName')#" width="187" height="52" style="align:left; vertical-align:top; border:0px" />
            </a>
        </td>
        <td style="width:100%; background-image:url(themes/Xaraya_Classic/images/header_fill.gif)">
            <img src="themes/Xaraya_Classic/images/dot.gif" alt="" style="align:middle; vertical-align:top; width:1px; height:1px; border:0px" />
        </td>
        <td style="align:right; background-image:url(themes/Xaraya_Classic/images/header_fill.gif)">
            <a href="http://www.xaraya.com">
            <img src="themes/Xaraya_Classic/images/header_logo2.gif" alt="#xarModGetVar('themes', 'SiteName')#" style="vertical-align:top; width:259; height:52" />
            </a>			
        </td>
    </tr>
