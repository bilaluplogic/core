    <tr>
        <td align="center" colspan="3">-->
            <!--xar:blockgroup name="top" id="top" /-->
        <!--</td>
    </tr>
-->
</table>

<!-- start body -->

<table width="100%" cellpadding="0" cellspacing="0" border="0">
    <tr>
        <td style="width: 18%;" align="left" valign="top">
            <table width="100%" cellpadding="0" cellspacing="0" border="0">
                <tr>
                    <td style="background-color:##ffffff;">
                        <xar:blockgroup name="left" id="left" />
                    </td>
                </tr>
                <tr style="height:200px;">
                    <td style="background-image: url(themes/Xaraya_Classic/images/sideline.gif)" >
                        <img src="themes/Xaraya_Classic/images/dot.gif" alt="" />
                    </td>
                </tr>
            </table>
        </td>
        <td style="width:1%; background-image: url(themes/Xaraya_Classic/images/vline.gif);">
			<img src="themes/Xaraya_Classic/images/dot.gif" alt="" style="align:middle; vertical-align:top; width:2px; height:1px; border:0px" />
        </td>
        <td  class="main_copy" style="width: 80%; align: center; vertical-align: top">
            <xar:blockgroup name="center" id="center" />
            <xar:module id="modulespace" main="true" />
        </td>
        <td style="width: 1%"></td>
        <td style="width: 20%; align: left; vertical-align: top; background-image:url(themes/Xaraya_Classic/images/side_all.gif)">
            <xar:blockgroup name="right" id="right" />
        </td>
    </tr>
</table>
<table width="100%" cellpadding="0" cellspacing="0">
    <tr>
        <td style="height:20px; text-align: right; padding-right: 10px; background-color:##f0f0f0">
            <b>#xarModGetVar('themes', 'SiteSlogan')#</b>
        </td>
    </tr>
</table>
<div style="text-align: center;">
<b>#xarModGetVar('themes', 'SiteCopyRight')#</b>
<br />
<br />
#xarModGetVar('themes', 'SiteFooter')#
</div>
